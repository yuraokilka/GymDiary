@model GymDiary.WebUI.ViewModels.ExerciseViewModel

<div class="row">
    <div class="col-md-4">
        <p>@Model.ExerciseType.Name</p>
    </div>
    <div class="col-md-8">
        <div class="row">
            <div class="col-md-12">
                @{
                    foreach (var set in Model.Sets)
                    {
                        @Html.Partial("_SetPartial", set)
                    }
                }
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" style="padding-top: 10px;"><em>@Model.Comment</em></div>
        </div>
    </div>
</div>